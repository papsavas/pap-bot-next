{
  "name": "bot",
  "version": "0.1.0",
  "description": "Multipurporsed Discord Bot",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && node dist/src/index.js",
    "dev": "ts-node src/index.ts && ts-node src/server.ts",
    "registerCommand": "node --loader ts-node/esm ./src/scripts/registerCommand",
    "unregisterCommand": "node --loader ts-node/esm ./src/scripts/deleteCommand",
    "migrateChannel": "node --loader ts-node/esm ./src/scripts/migrateChannel",
    "syncGuilds": "node --loader ts-node/esm ./src/scripts/syncDBguilds"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "database": "*",
    "discord.js": "^14.8.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "ts-node": "^10.9.1"
  }
}